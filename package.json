{
  "name": "oldschool3d",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html"
  },
  "dependencies": {
    "deepmerge": "4.2.2",
    "gl-matrix": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "prefer-const": [
        "error"
      ],
      "no-var": [
        "error"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "keywords": [],
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}